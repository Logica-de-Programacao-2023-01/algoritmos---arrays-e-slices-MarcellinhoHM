// Crie um Array bidimensional de inteiros com 3 linhas e 2 colunas.
// Solicite ao usuário que informe os valores de cada elemento da matriz. Em seguida, imprima a matriz resultante.
package main

import "fmt"

func main() {
	var matriz [3][2]int
	fmt.Println(matriz)

	var n1 int
	var n2 int
	var n3 int
	var n4 int
	var n5 int
	var n6 int

	fmt.Print("Escolha um número.")
	fmt.Scan(&n1)
	fmt.Print("Escolha um número.")
	fmt.Scan(&n2)
	fmt.Print("Escolha um número.")
	fmt.Scan(&n3)
	fmt.Print("Escolha um número.")
	fmt.Scan(&n4)
	fmt.Print("Escolha um número.")
	fmt.Scan(&n5)
	fmt.Print("Escolha um número.")
	fmt.Scan(&n6)

	matrizin := [3][2]int{{n1, n2}, {n3, n4}, {n5, n6}}
	fmt.Println(matrizin)
	
}
